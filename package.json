{
  "name": "angular.dev",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "json-server": "json-server --watch db.json",
    "dev": "concurrently \"ng serve\" \"json-server --watch db.json\"",
    "dev:verbose": "concurrently --names \"ANGULAR,JSON-SERVER\" --prefix-colors \"cyan,magenta\" \"ng serve\" \"json-server --watch db.json\""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0-next",
    "@angular/compiler": "^20.1.0-next",
    "@angular/core": "^20.1.0-next",
    "@angular/forms": "^20.1.0-next",
    "@angular/platform-browser": "^20.1.0-next",
    "@angular/router": "^20.1.0-next",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.0-next",
    "@angular/cli": "^20.1.0-next",
    "@angular/compiler-cli": "^20.1.0-next",
    "@types/node": "^16.11.35",
    "concurrently": "^8.2.2",
    "json-server": "^1.0.0-beta.3",
    "ts-node": "~10.9.0",
    "typescript": "~5.8.0"
  }
}
